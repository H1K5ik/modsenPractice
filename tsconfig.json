{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@decorators": [
        "src/libs/decorators"
      ],
      "@decorators/*": [
        "src/libs/decorators/*"
      ],
      "@config": [
        "src/libs/config"
      ],
      "@config/*": [
        "src/libs/config/*"
      ],
      "@prisma": [
        "src/libs/prisma"
      ],
      "@prisma/*": [
        "src/libs/prisma/*"
      ],
      "@modules": [
        "src/modules"
      ],
      "@modules/*": [
        "src/modules/*"
      ],
      "@dto": [
        "src/libs/dto"
      ],
      "@dto/*": [
        "src/libs/dto/*"
      ],
      "@constants": [
        "src/libs/constants"
      ],
      "@constants/*": [
        "src/libs/constants/*"
      ],
      "@utils": [
        "src/libs/utils"
      ],
      "@utils/*": [
        "src/libs/utils/*"
      ],
      "@interfaces": [
        "src/libs/interfaces"
      ],
      "@interfaces/*": [
        "src/libs/interfaces/*"
      ],
    }
  }
}
